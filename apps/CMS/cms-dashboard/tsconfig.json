{
  "extends": "../../../tsconfig.base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "baseUrl": "../../..",
    "paths": {
      "@/app/*": ["apps/CMS/cms-dashboard/src/app/*"],
      "@/generated": ["apps/CMS/cms-dashboard/src/generated/index.ts"],
      "@/common/*": ["apps/CMS/cms-dashboard/src/common/*"],
      "@/file-management": ["libs/file-management/src/index.ts"],
      "@/icons": ["apps/CMS/cms-dashboard/src/assets/icons/index.ts"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ],
    "types": ["jest", "node"]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    "../../../apps/CMS/cms-dashboard/.next/types/**/*.ts",
    "../../../dist/apps/CMS/cms-dashboard/.next/types/**/*.ts",
    "next-env.d.ts",
    "../../../apps/CMS/cms-dashboard/.next/types/**/*.ts",
    "../../../dist/apps/CMS/cms-dashboard/.next/types/**/*.ts",
    ".next/types/**/*.ts",
    "specs/dashboard"
  ],
  "exclude": ["node_modules", "jest.config.ts"]
}
