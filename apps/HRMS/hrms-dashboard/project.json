{
  "name": "hrms-dashboard",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/HRMS/hrms-dashboard",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "local",
      "options": {
        "outputPath": "dist/apps/HRMS/hrms-dashboard"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/HRMS/hrms-dashboard",
          "envFiles": [".env.development"]
        },
        "preview": {
          "envFiles": [".env.preview"]
        },
        "local": {
          "envFiles": [".env.local"]
        },
        "production": {
          "envFiles": [".env.production"]
        }
      },
      "dependsOn": [
        {
          "target": "clean",
          "params": "forward"
        },
        {
          "target": "codegen",
          "params": "forward"
        }
      ]
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "hrms-dashboard:build",
        "dev": true,
        "envFiles": [".env.development"]
      },
      "configurations": {
        "development": {
          "buildTarget": "hrms-dashboard:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "hrms-dashboard:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "hrms-dashboard:build:production"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["npx nx codegen hrms-dashboard --configuration=development", "npx env-cmd -f apps/HRMS/hrms-dashboard/.env.development nx serve hrms-dashboard --configuration=development"],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean",
          "params": "forward"
        }
      ]
    },
    "dev-local": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["npx nx codegen dashboard --configuration=local", "npx env-cmd -f apps/HRMS/hrms-dashboard/.env.local nx serve hrms-dashboard"],
        "parallel": false
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/HRMS/hrms-dashboard/jest.config.ts",
        "codeCoverage": true
      },
      "configurations": {
        "ci": {
          "ci": true
        }
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["rm -rf dist/apps/HRMS/hrms-dashboard", "rm -rf .vercel"]
      }
    },
    "export-env": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["export $(cat apps/HRMS/hrms-dashboard/.env | xargs)"],
        "parallel": false
      }
    },

    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/HRMS/hrms-dashboard/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "cypress:run": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx nx cypress hrms-dashboard",
          "npx mochawesome-merge --reportDir apps/HRMS/hrms-dashboard/cypress/results/*.json -o apps/HRMS/hrms-dashboard/.cy/report.json",
          "npx marge apps/HRMS/hrms-dashboard/.cy/report.json -o apps/HRMS/hrms-dashboard/.cy",
          "npx nx check-cypress-code-coverage scripts apps/HRMS/hrms-dashboard"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "cypress:clean"
        },
        {
          "target": "codegen"
        }
      ]
    },
    "cypress:clean": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["rm -rf apps/HRMS/hrms-dashboard/cypress/results", "rm -rf apps/HRMS/hrms-dashboard/.nyc_output", "rm -rf apps/HRMS/hrms-dashboard/.cy"],
        "parallel": false
      }
    },
    "cypress": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/HRMS/hrms-dashboard/cypress.config.ts",
        "testingType": "e2e",
        "reporter": "mochawesome",
        "devServerTarget": "hrms-dashboard:serve:development"
      }
    },
    "cypress:open": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/HRMS/hrms-dashboard/cypress.config.ts",
        "testingType": "e2e",
        "reporter": "mochawesome",
        "devServerTarget": "hrms-dashboard:serve:development",
        "watch": true
      },
      "dependsOn": [
        {
          "target": "cypress:clean"
        },
        {
          "target": "codegen"
        }
      ]
    },
    "codegen": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "development",
      "configurations": {
        "local": {
          "commands": ["npx env-cmd -f apps/HRMS/hrms-dashboard/.env.local graphql-codegen --config ./apps/HRMS/hrms-dashboard/graphql-codegen.yml"]
        },
        "development": {
          "commands": ["npx env-cmd -f apps/HRMS/hrms-dashboard/.env.development graphql-codegen --config ./apps/HRMS/hrms-dashboard/graphql-codegen.yml"]
        },
        "preview": {
          "commands": ["npx env-cmd -f apps/HRMS/hrms-dashboard/.env.preview graphql-codegen --config ./apps/HRMS/hrms-dashboard/graphql-codegen.yml"]
        },
        "production": {
          "commands": ["npx env-cmd -f apps/HRMS/hrms-dashboard/.env.production graphql-codegen --config ./apps/HRMS/hrms-dashboard/graphql-codegen.yml"]
        }
      }
    },
    "get-secrets": {
      "executor": "@pinecone-intern-monorepo/secrets:get",
      "defaultConfiguration": "production",
      "configurations": {
        "production": {
          "groups": ["hrms-dashboard", "file-management"],
          "env": "prod"
        }
      }
    },
    "add-secrets": {
      "executor": "@pinecone-intern-monorepo/secrets:add"
    },
    "preview-all": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx nx codegen hrms-dashboard --configuration=preview",
          "npx dotenv -v VERCEL_ORG_ID=team_0ASDilhqwPl5fll9OnzqDM30 -v VERCEL_PROJECT_ID=prj_N6bIYczHoE9hntCfwQkzroFzDDQn -- vercel pull --yes --environment=preview --token=$VERCEL_TOKEN",
          "npx env-cmd -f apps/HRMS/hrms-dashboard/.env.preview vercel build --token=$VERCEL_TOKEN",
          "npx dotenv -v VERCEL_ORG_ID=team_0ASDilhqwPl5fll9OnzqDM30 -v VERCEL_PROJECT_ID=prj_N6bIYczHoE9hntCfwQkzroFzDDQn -- vercel --archive=tgz --prebuilt --token=$VERCEL_TOKEN"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean"
        },
        {
          "target": "get-secrets"
        },
        {
          "target": "export-env"
        }
      ]
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx nx codegen hrms-dashboard --configuration=development",
          "npx dotenv -v VERCEL_ORG_ID=team_0ASDilhqwPl5fll9OnzqDM30 -v VERCEL_PROJECT_ID=prj_N6bIYczHoE9hntCfwQkzroFzDDQn -- vercel pull --yes --environment=preview --token=$VERCEL_TOKEN",
          "npx env-cmd -f apps/HRMS/hrms-dashboard/.env.development vercel build --token=$VERCEL_TOKEN",
          "npx dotenv -v VERCEL_ORG_ID=team_0ASDilhqwPl5fll9OnzqDM30 -v VERCEL_PROJECT_ID=prj_N6bIYczHoE9hntCfwQkzroFzDDQn -- vercel --archive=tgz --prebuilt --token=$VERCEL_TOKEN"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean"
        },
        {
          "target": "get-secrets"
        },
        {
          "target": "export-env"
        }
      ]
    },
    "deploy-prod": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx nx codegen hrms-dashboard --configuration=production",
          "npx dotenv -v VERCEL_ORG_ID=team_0ASDilhqwPl5fll9OnzqDM30 -v VERCEL_PROJECT_ID=prj_N6bIYczHoE9hntCfwQkzroFzDDQn -- vercel pull --yes --environment=production --token=$VERCEL_TOKEN",
          "npx env-cmd -f apps/HRMS/hrms-dashboard/.env.production vercel build --token=$VERCEL_TOKEN",
          "npx dotenv -v VERCEL_ORG_ID=team_0ASDilhqwPl5fll9OnzqDM30 -v VERCEL_PROJECT_ID=prj_N6bIYczHoE9hntCfwQkzroFzDDQn -- vercel --archive=tgz --prebuilt --prod --token=$VERCEL_TOKEN"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean"
        },
        {
          "target": "get-secrets"
        },
        {
          "target": "export-env"
        }
      ]
    }
  },
  "tags": []
}
