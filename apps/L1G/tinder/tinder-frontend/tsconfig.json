{
  "extends": "../../../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "types": ["jest", "@testing-library/jest-dom"],
    "jsx": "preserve",
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/components/ui/*": ["../../../../libs/shadcn/src/lib/ui/*"],
      "@/*": ["src/*"]
    },

    "types": ["jest", "node"]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    "../../../../apps/L1G/tinder/tinder-frontend/.next/types/**/*.ts",
    "../../../../dist/apps/L1G/tinder/tinder-frontend/.next/types/**/*.ts",
    "next-env.d.ts",
    "src/graphql/hello-query.graphql"
  ],
  "exclude": ["node_modules", "jest.config.ts", "src/**/*.spec.ts", "src/**/*.test.ts"]
}
