{
  "name": "server",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/example/server",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/example/server"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/example/server"
        },
        "production": {}
      },
      "dependsOn": [{ "target": "graphql-codegen" }]
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "server:build",
        "dev": true,
        "port": 8000
      },
      "configurations": {
        "development": {
          "buildTarget": "server:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "server:build:production",
          "dev": false
        }
      },
      "dependsOn": [
        {
          "target": "graphql-codegen:local"
        }
      ]
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "server:build:production"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/example/server/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "graphql-codegen:local": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["graphql-codegen --config apps/example/server/codegen.ts"]
      },
      "dependsOn": [
        {
          "target": "get-secrets:development"
        }
      ]
    },
    "graphql-codegen": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["graphql-codegen --config apps/example/server/codegen.ts"]
      },
      "dependsOn": [
        {
          "target": "get-secrets:testing"
        }
      ]
    },
    "get-secrets:development": {
      "executor": "@pinecone-intern-monorepo/secrets:get",
      "defaultConfiguration": "development",
      "configurations": {
        "development": {
          "groups": ["server"],
          "env": "dev"
        }
      }
    },
    "get-secrets:testing": {
      "executor": "@pinecone-intern-monorepo/secrets:get",
      "defaultConfiguration": "testing",
      "configurations": {
        "testing": {
          "groups": ["server"],
          "env": "test"
        }
      }
    },
    "get-secrets:production": {
      "executor": "@pinecone-intern-monorepo/secrets:get",
      "defaultConfiguration": "production",
      "configurations": {
        "production": {
          "groups": ["server"],
          "env": "prod"
        }
      }
    },
    "add-secrets": {
      "executor": "@pinecone-intern-monorepo/secrets:add"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/example/server/jest.config.ts",
        "codeCoverage": true,
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true
        }
      },
      "dependsOn": [
        {
          "target": "graphql-codegen"
        }
      ]
    },
    "export-env": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["export $(cat apps/example/server/.env | xargs)"],
        "parallel": false
      }
    },
    "deploy-dev": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx dotenv -v VERCEL_ORG_ID=team_DqGGgGmMFvEQF0TUpV2NV3WY -v VERCEL_PROJECT_ID=prj_3ibNtLp9PrLqRLPSiuBi3dtjvpnZ -- vercel pull --yes --environment=production --token=$VERCEL_TOKEN",
          "vercel build --prod --token=$VERCEL_TOKEN",
          "npx dotenv -v VERCEL_ORG_ID=team_DqGGgGmMFvEQF0TUpV2NV3WY -v VERCEL_PROJECT_ID=prj_3ibNtLp9PrLqRLPSiuBi3dtjvpnZ -- vercel --prebuilt --prod --token=$VERCEL_TOKEN"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean"
        },
        {
          "target": "graphql-codegen:local"
        },
        {
          "target": "export-env"
        }
      ]
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx dotenv -v VERCEL_ORG_ID=team_DqGGgGmMFvEQF0TUpV2NV3WY -v VERCEL_PROJECT_ID=prj_dRwZTK5gVQeuIcYjV35t3r0muSBC -- vercel pull --yes --environment=preview --token=$VERCEL_TOKEN",
          "vercel build --token=$VERCEL_TOKEN",
          "npx dotenv -v VERCEL_ORG_ID=team_DqGGgGmMFvEQF0TUpV2NV3WY -v VERCEL_PROJECT_ID=prj_dRwZTK5gVQeuIcYjV35t3r0muSBC -- vercel --prebuilt --token=$VERCEL_TOKEN"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean"
        },
        {
          "target": "graphql-codegen"
        },
        {
          "target": "export-env"
        }
      ]
    },
    "deploy-testing": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx dotenv -v VERCEL_ORG_ID=team_DqGGgGmMFvEQF0TUpV2NV3WY -v VERCEL_PROJECT_ID=prj_dRwZTK5gVQeuIcYjV35t3r0muSBC -- vercel pull --yes --environment=production --token=$VERCEL_TOKEN",
          "vercel build --prod --token=$VERCEL_TOKEN",
          "npx dotenv -v VERCEL_ORG_ID=team_DqGGgGmMFvEQF0TUpV2NV3WY -v VERCEL_PROJECT_ID=prj_dRwZTK5gVQeuIcYjV35t3r0muSBC -- vercel --prebuilt --prod --token=$VERCEL_TOKEN"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean"
        },
        {
          "target": "graphql-codegen"
        },
        {
          "target": "export-env"
        }
      ]
    },
    "deploy-prod": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx dotenv -v VERCEL_ORG_ID=team_DqGGgGmMFvEQF0TUpV2NV3WY -v VERCEL_PROJECT_ID=prj_AfmuJxq4AgAqYQa2wsD5mtXXEJ5d -- vercel pull --yes --environment=production --token=$VERCEL_TOKEN",
          "vercel build --prod --token=$VERCEL_TOKEN",
          "npx dotenv -v VERCEL_ORG_ID=team_DqGGgGmMFvEQF0TUpV2NV3WY -v VERCEL_PROJECT_ID=prj_AfmuJxq4AgAqYQa2wsD5mtXXEJ5d -- vercel --prebuilt --prod --token=$VERCEL_TOKEN"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean"
        },
        {
          "target": "get-secrets:production"
        },
        {
          "target": "export-env"
        },
        {
          "target": "graphql-codegen"
        }
      ]
    }
  },
  "tags": []
}
