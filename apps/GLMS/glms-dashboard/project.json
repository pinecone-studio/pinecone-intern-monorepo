{
  "name": "glms-dashboard",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/GLMS/glms-dashboard",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/GLMS/glms-dashboard"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/GLMS/glms-dashboard"
        },
        "production": {}
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/GLMS/glms-dashboard/jest.config.ts",
        "codeCoverage": true
      },
      "configurations": {
        "ci": {
          "ci": true
        }
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "glms-dashboard:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "glms-dashboard:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "glms-dashboard:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "glms-dashboard:build:production"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/GLMS/glms-dashboard/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "cypress:run": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "bunx nx cypress:clean glms-dashboard",
          "bunx nx cypress:test glms-dashboard",
          "bunx mochawesome-merge --reportDir apps/GLMS/glms-dashboard/cypress/results/*.json -o apps/GLMS/glms-dashboard/.cy/report.json",
          "bunx marge apps/GLMS/glms-dashboard/.cy/report.json -o apps/GLMS/glms-dashboard/.cy"
        ],
        "parallel": false
      }
    },
    "cypress:clean": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "rm -rf apps/GLMS/glms-dashboard/cypress/results",
          "rm -rf apps/GLMS/glms-dashboard/.nyc_output",
          "rm -rf apps/GLMS/glms-dashboard/.cy"
        ],
        "parallel": false
      }
    },
    "cypress:test": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/GLMS/glms-dashboard/cypress.config.ts",
        "testingType": "e2e",
        "reporter": "mochawesome",
        "devServerTarget": "glms-dashboard:serve:development"
      }
    },
    "cypress:dev": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/GLMS/glms-dashboard/cypress.config.ts",
        "testingType": "e2e",
        "reporter": "mochawesome",
        "devServerTarget": "glms-dashboard:serve:development",
        "watch": true
      }
    }
  },
  "tags": []
}
