{
  "name": "leaderboard-service",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/GLMS/leaderboard-service",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/GLMS/leaderboard-service"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/GLMS/leaderboard-service"
        },
        "production": {}
      },
      "dependsOn": [{ "target": "graphql-codegen" }]
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "leaderboard-service:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "leaderboard-service:build:development",
          "dev": true,
          "port": 4400
        },
        "production": {
          "buildTarget": "leaderboard-service:build:production",
          "dev": false
        }
      },
      "dependsOn": [{ "target": "graphql-codegen" }]
    },
    "graphql-codegen": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "graphql-codegen --config apps/GLMS/leaderboard-service/codegen.ts"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/GLMS/leaderboard-service/jest.config.ts",
        "codeCoverage": true,
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "leaderboard-service:build:production"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/GLMS/leaderboard-service/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "rm -rf .vercel",
          "rm -rf dist/apps/GLMS/leaderboard-service"
        ]
      }
    },
    "export-env": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "export $(cat apps/GLMS/leaderboard-service/.env | xargs)"
        ],
        "parallel": false
      }
    },
    "deploy-dev": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx dotenv -v VERCEL_ORG_ID=team_DqGGgGmMFvEQF0TUpV2NV3WY -v VERCEL_PROJECT_ID=prj_wDggR2Vlpawyr1L6aMpriaiuOGuI -- vercel pull --yes --environment=production --token=$VERCEL_TOKEN",
          "vercel build --prod --token=$VERCEL_TOKEN",
          "npx dotenv -v VERCEL_ORG_ID=team_DqGGgGmMFvEQF0TUpV2NV3WY -v VERCEL_PROJECT_ID=prj_wDggR2Vlpawyr1L6aMpriaiuOGuI -- vercel --prebuilt --prod --token=$VERCEL_TOKEN"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean"
        },
        {
          "target": "export-env"
        }
      ]
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx dotenv -v VERCEL_ORG_ID=team_DqGGgGmMFvEQF0TUpV2NV3WY -v VERCEL_PROJECT_ID=prj_scmkCclpjnLvU7Nb1niNTrYzTDT3 -- vercel pull --yes --environment=preview --token=$VERCEL_TOKEN",
          "vercel build --token=$VERCEL_TOKEN",
          "npx dotenv -v VERCEL_ORG_ID=team_DqGGgGmMFvEQF0TUpV2NV3WY -v VERCEL_PROJECT_ID=prj_scmkCclpjnLvU7Nb1niNTrYzTDT3 -- vercel --prebuilt --token=$VERCEL_TOKEN"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean"
        },
        {
          "target": "export-env"
        }
      ]
    },
    "deploy-testing": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx dotenv -v VERCEL_ORG_ID=team_DqGGgGmMFvEQF0TUpV2NV3WY -v VERCEL_PROJECT_ID=prj_scmkCclpjnLvU7Nb1niNTrYzTDT3 -- vercel pull --yes --environment=production --token=$VERCEL_TOKEN",
          "vercel build --prod --token=$VERCEL_TOKEN",
          "npx dotenv -v VERCEL_ORG_ID=team_DqGGgGmMFvEQF0TUpV2NV3WY -v VERCEL_PROJECT_ID=prj_scmkCclpjnLvU7Nb1niNTrYzTDT3 -- vercel --prebuilt --prod --token=$VERCEL_TOKEN"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean"
        },
        {
          "target": "export-env"
        }
      ]
    },
    "deploy-prod": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx dotenv -v VERCEL_ORG_ID=team_DqGGgGmMFvEQF0TUpV2NV3WY -v VERCEL_PROJECT_ID=prj_eZCCJs6Ji403rYqiPjVPPTGNB2Cl -- vercel pull --yes --environment=production --token=$VERCEL_TOKEN",
          "vercel build --prod --token=$VERCEL_TOKEN",
          "npx dotenv -v VERCEL_ORG_ID=team_DqGGgGmMFvEQF0TUpV2NV3WY -v VERCEL_PROJECT_ID=prj_eZCCJs6Ji403rYqiPjVPPTGNB2Cl -- vercel --prebuilt --prod --token=$VERCEL_TOKEN"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean"
        },
        {
          "target": "export-env"
        }
      ]
    }
  },
  "tags": []
}
