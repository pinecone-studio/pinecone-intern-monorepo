{
  "name": "profile-service",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/GLMS/profile-service",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/GLMS/profile-service"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/GLMS/profile-service"
        },
        "production": {}
      },
      "dependsOn": [{ "target": "graphql-codegen" }]
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "profile-service:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "profile-service:build:development",
          "dev": true,
          "port": 4500
        },
        "production": {
          "buildTarget": "profile-service:build:production",
          "dev": false
        }
      },
      "dependsOn": [{ "target": "graphql-codegen" }]
    },
    "graphql-codegen": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "graphql-codegen --config apps/GLMS/profile-service/codegen.ts"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/GLMS/profile-service/jest.config.ts",
        "codeCoverage": true,
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true
        }
      }
    },

    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "profile-service:build:production"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/GLMS/profile-service/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "get-secrets": {
      "executor": "@pinecone-intern-monorepo/secrets:get",
      "defaultConfiguration": "development",
      "configurations": {
        "development": {
          "groups": ["profile-service"],
          "env": "dev"
        },
        "production": {
          "groups": ["profile-service"],
          "env": "prod"
        },
        "testing": {
          "groups": ["profile-service"],
          "env": "test"
        }
      }
    },
    "add-secrets": {
      "executor": "@pinecone-intern-monorepo/secrets:add"
    }
  },
  "tags": []
}
