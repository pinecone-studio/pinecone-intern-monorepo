{
  "name": "instagram-web",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/L2A/instagram/web",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/L2A/instagram/web"
      },
      "configurations": {
        "development": {
          "outputPath": "dist/apps/L2A/instagram/web"
        }
      },
      "dependsOn": [
        {
          "target": "codegen:dev",
          "params": "forward"
        }
      ]
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "instagram-web:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "instagram-web:build:development",
          "dev": true,
          "port": 4201
        },
        "production": {
          "buildTarget": "instagram-web:build:production",
          "dev": false
        }
      },
      "dependsOn": [
        {
          "target": "codegen:dev",
          "params": "forward"
        },
        {
          "target": "export-env",
          "params": "forward"
        }
      ]
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "instagram-web:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "{projectRoot}/jest.config.ts",
        "passWithNoTests": true,
        "codeCoverage": true
      },
      "dependsOn": [
        {
          "target": "codegen:testing",
          "params": "forward"
        }
      ]
    },
    "get-secrets:dev": {
      "executor": "@pinecone-intern-monorepo/secrets:get",
      "defaultConfiguration": "development",
      "configurations": {
        "development": {
          "groups": ["instagram-web"],
          "env": "dev"
        }
      }
    },
    "get-secrets:testing": {
      "executor": "@pinecone-intern-monorepo/secrets:get",
      "defaultConfiguration": "testing",
      "configurations": {
        "testing": {
          "groups": ["instagram-web"],
          "env": "test"
        }
      }
    },
    "get-secrets:prod": {
      "executor": "@pinecone-intern-monorepo/secrets:get",
      "defaultConfiguration": "prod",
      "configurations": {
        "prod": {
          "groups": ["instagram-web"],
          "env": "prod"
        }
      }
    },
    "export-env": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["export $(cat apps/L2A/instagram/web/.env | xargs)"],
        "parallel": false
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["rm -rf dist/apps/L2A/instagram/web", "rm -rf .vercel"]
      }
    },
    "e2e": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["npx nx cypress example-frontend"],
        "parallel": false
      },
      "dependsOn": [{ "target": "cypress:clean" }]
    },
    "e2e:open": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["npx nx cypress instagram-web --watch"],
        "parallel": false
      },
      "dependsOn": [{ "target": "cypress:clean" }]
    },
    "cypress:clean": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["rm -rf apps/L2A/instagram/web/cypress/results", "rm -rf apps/L2A/instagram/web/.nyc_output", "rm -rf apps/L2A/instagram/web/.cy", "rm -rf apps/L2A/instagram/web/coverage"],
        "parallel": false
      }
    },
    "cypress": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/L2A/instagram/web/cypress.config.ts",
        "testingType": "e2e",
        "reporter": "mochawesome",
        "devServerTarget": "instagram-web:serve:development",
        "browser": "chrome"
      },
      "dependsOn": [
        {
          "target": "cypress-generate-all-page-json"
        },
        {
          "target": "get-secrets"
        },
        {
          "target": "export-env"
        }
      ]
    },
    "cypress-generate-all-page-json": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["npx ts-node apps/L2A/instagram/web/cypress/utils/get-all-pages.ts"],
        "parallel": false
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx dotenv -v VERCEL_ORG_ID=team_0ASDilhqwPl5fll9OnzqDM30 -v VERCEL_PROJECT_ID= -- vercel pull --yes --environment=preview --token=$VERCEL_TOKEN",
          "npx env-cmd -f apps/L2A/instagram/web/.env vercel build --token=$VERCEL_TOKEN",
          "npx dotenv -v VERCEL_ORG_ID=team_0ASDilhqwPl5fll9OnzqDM30 -v VERCEL_PROJECT_ID= -- vercel --archive=tgz --prebuilt --token=$VERCEL_TOKEN"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean"
        },
        {
          "target": "get-secrets:testing"
        },
        {
          "target": "export-env"
        },
        {
          "target": "codegen:testing",
          "params": "forward"
        }
      ]
    },
    "deploy-dev": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx dotenv -v VERCEL_ORG_ID=team_0ASDilhqwPl5fll9OnzqDM30 -v VERCEL_PROJECT_ID= -- vercel pull --yes --environment=production --token=$VERCEL_TOKEN",
          "vercel build --prod --token=$VERCEL_TOKEN",
          "npx dotenv -v VERCEL_ORG_ID=team_0ASDilhqwPl5fll9OnzqDM30 -v VERCEL_PROJECT_ID= -- vercel --prebuilt --prod --token=$VERCEL_TOKEN"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean"
        },
        {
          "target": "get-secrets:dev"
        },
        {
          "target": "export-env"
        },
        {
          "target": "codegen:dev",
          "params": "forward"
        }
      ]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/L2A/instagram/web/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "codegen:prod": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["graphql-codegen --config ./apps/L2A/instagram/web/codegen.ts"]
      },
      "dependsOn": [
        {
          "target": "get-secrets:prod"
        }
      ]
    },
    "codegen:dev": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["graphql-codegen --config ./apps/L2A/instagram/web/codegen.ts"]
      },
      "dependsOn": [{ "target": "get-secrets:dev" }]
    },
    "codegen:testing": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["graphql-codegen --config ./apps/L2A/instagram/web/codegen.ts"]
      },
      "dependsOn": [
        {
          "target": "get-secrets:testing"
        }
      ]
    },
    "deploy-prod": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx dotenv -v VERCEL_ORG_ID=team_0ASDilhqwPl5fll9OnzqDM30 -v VERCEL_PROJECT_ID= -- vercel pull --yes --environment=production --token=$VERCEL_TOKEN",
          "vercel build --prod --token=$VERCEL_TOKEN",
          "npx dotenv -v VERCEL_ORG_ID=team_0ASDilhqwPl5fll9OnzqDM30 -v VERCEL_PROJECT_ID= -- vercel --prebuilt --prod --token=$VERCEL_TOKEN"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean"
        },
        {
          "target": "get-secrets:prod"
        },
        {
          "target": "export-env"
        },
        {
          "target": "codegen:prod",
          "params": "forward"
        }
      ]
    }
  },
  "tags": []
}
